package cwi;

public class Node {
	private String value;
	private Node next;
		public Node(String value,Node next)
		{
			this.value = value;
			this.next = next;
		}
		public Node(String value){
			this.value = value;
		}
		public String getValue(){
			return value;
		}
		public Node getnext(){
			return this.next;
		}
		public void remove(){
			next=null;
		}
		public void setNext(Node next){
			this.next = next;
		}
}
