package Test;

import static org.junit.Assert.*;

import org.junit.Test;

import cwi.LinkedList;

public class Testes {

	@Test
	public void adicionarPrimeiroNaLista() {
		LinkedList linkesList = new LinkedList();
		linkesList.addFirst("primeiro");
		assertEquals(linkesList.getFirst(),"primeiro");
	}

}
